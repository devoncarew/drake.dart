{
  "name": "__MSG_app_name__",
  "description" : "__MSG_app_description__",

  "version": "0.6.19.0",
  "minimum_chrome_version": "27",
  "manifest_version": 2,

  "default_locale": "en",

  "icons": {
    "128": "images/drake.png"
  },

  "app": {
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    }
  },

  "permissions": [
    "clipboardRead",
    "clipboardWrite", {
      "fileSystem": ["write"]
    },
    { "socket" : ["tcp-connect", "tcp-listen", "udp-bind", "udp-send-to"] },
    "storage",
    "webview"
  ],

  "file_handlers": {
    "text": {
      "types": [
        "application/dart",
        "application/javascript",
        "text/css",
        "text/dart",
        "text/html",
        "text/javascript",
        "text/json",
        "text/xml",
        "text/yaml"
      ],
      "title": "Web Editor"
    }
  }

}
